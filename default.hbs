	<link rel="apple-touch-icon" sizes="114x114" href="/assets/images/apple-touch-icon-114x114.png?v=PYYqxk4Ozz">
	<link rel="apple-touch-icon" sizes="120x120" href="/assets/images/apple-touch-icon-120x120.png?v=PYYqxk4Ozz">
	<link rel="apple-touch-icon" sizes="144x144" href="/assets/images/apple-touch-icon-144x144.png?v=PYYqxk4Ozz">
	<link rel="apple-touch-icon" sizes="152x152" href="/assets/images/apple-touch-icon-152x152.png?v=PYYqxk4Ozz">
	<link rel="apple-touch-icon" sizes="180x180" href="/assets/images/apple-touch-icon-180x180.png?v=PYYqxk4Ozz">
	<link rel="icon" type="image/png" href="/assets/images/favicon-32x32.png?v=PYYqxk4Ozz" sizes="32x32">
	<link rel="icon" type="image/png" href="/assets/images/favicon-194x194.png?v=PYYqxk4Ozz" sizes="194x194">
	<link rel="icon" type="image/png" href="/assets/images/favicon-96x96.png?v=PYYqxk4Ozz" sizes="96x96">
	<link rel="icon" type="image/png" href="/assets/images/android-chrome-192x192.png?v=PYYqxk4Ozz" sizes="192x192">
	<link rel="icon" type="image/png" href="/assets/images/favicon-16x16.png?v=PYYqxk4Ozz" sizes="16x16">
	<link rel="manifest" href="/assets/images/manifest.json?v=PYYqxk4Ozz">
	<link rel="shortcut icon" href="/assets/images/favicon.ico?v=PYYqxk4Ozz">
	<meta name="msapplication-TileColor" content="#da532c">
	<meta name="msapplication-TileImage" content="/assets/images/mstile-144x144.png?v=PYYqxk4Ozz">
	<meta name="msapplication-config" content="/assets/images/browserconfig.xml?v=PYYqxk4Ozz">
	<meta name="theme-color" content="#667889">

    {{! Ghost outputs important style and meta data with this tag }}
    {{ghost_head}}
</head>
<body class="{{body_class}}">
    {{> logo}}

    {{{body}}}

    {{> footer}}

    {{! Ghost outputs important scripts and data with this tag }}
    {{ghost_foot}}

    <script type="text/javascript">  
    if (typeof jQuery == 'undefined') {
  	document.write('<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></'+'script>');
    }
    </script>

    <script type="text/javascript" src="{{asset "js/jquery.fitvids.js"}}"></script>
    <script type="text/javascript" src="{{asset "js/index.js"}}"></script>

    <script type="text/javascript">
      {{> config}}

      (function(g,h,o,s,t,z){g.GoogleAnalyticsObject=s;g[s]||(g[s]=
      function(){(g[s].q=g[s].q||[]).push(arguments)});g[s].s=+new Date;
      t=h.createElement(o);z=h.getElementsByTagName(o)[0];
      t.src='//www.google-analytics.com/analytics.js';
      z.parentNode.insertBefore(t,z)}(window,document,'script','ga'));
      ga('create',ga_ua);ga('send','pageview');

      (function () {
      var s = document.createElement('script'); s.async = true;
      s.type = 'text/javascript';
      s.src = '//' + disqus_shortname + '.disqus.com/count.js';
      (document.getElementsByTagName('HEAD')[0] || document.getElementsByTagName('BODY')[0]).appendChild(s);
      }());
      
      jQuery(document).ready(function(){
            jQuery(document).fitVids();
      });
    </script>
</body>
</html>
